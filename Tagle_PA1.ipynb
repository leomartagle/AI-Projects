   "source": [
    "def read(filename):\n",
    "    with open(filename, 'r') as f:\n",
    "        lines = f.read().split()\n",
    "\n",
    "    start_index = lines.index('start') + 1\n",
    "    goal_index = lines.index('goal') + 1\n",
    "\n",
    "    start = tuple(0 if x == '*' else int(x)\n",
    "                  for x in lines[start_index:start_index+9])\n",
    "    \n",
    "    goal = tuple(0 if x == '*' else int(x)\n",
    "                 for x in lines[goal_index:goal_index+9])\n",
    "\n",
    "    return start, goal"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "3029eb66-9f62-436d-851f-065300e8f6a0",
   "metadata": {},
   "outputs": [],
   "source": [
    "### Number of Misplaced Tiles\n",
    "\n",
    "def misplaced(state, goal):\n",
    "    distance = 0\n",
    "    count = 0\n",
    "    for i in range(9):\n",
    "        if state[i] != 0 and state[i] != goal[i]:\n",
    "            count += 1\n",
    "    return count\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "id": "7725ab5d-c56a-453f-bea7-ec607d2d0965",
   "metadata": {},
   "outputs": [],
   "source": [
    "### Manhattan Distance\n",
    "\n",
    "def manhattan(state, goal):\n",
    "    distance = 0\n",
    "    \n",
    "    for tile in range(1, 9):  # tiles 1 through 8 (ignore 0)\n",
    "        current_index = state.index(tile)\n",
    "        goal_index = goal.index(tile)\n",
    "        \n",
    "        current_row = current_index // 3\n",
    "        current_col = current_index % 3\n",
    "        \n",
    "        goal_row = goal_index // 3\n",
    "        goal_col = goal_index % 3\n",
    "        \n",
    "        distance = distance + abs(current_row - goal_row) + abs(current_col - goal_col)\n",
    "    \n",
    "    return distance"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "id": "f5591017-8525-4814-97f4-7e2d4305b1d1",
   "metadata": {},
   "outputs": [],
   "source": [
    "### Nilsson's Sequence Score\n",
    "def nilsson(state, goal):\n",
    "    P = manhattan(state, goal) #Manhattan Distance\n",
    "\n",
    "    border = [0,1,2,5,8,7,6,3] #outer tiles in clockwise order\n",
    "\n",
    "    S = 0\n",
    "\n",
    "    for i in range(len(border)):\n",
    "        current_tile = state[border[i]]\n",
    "        next_tile = state[border[(i+1) % 8]]  # wrap around\n",
    "\n",
    "        if current_tile != 0:  # ignore blank\n",
    "            correct_successor = current_tile + 1 if current_tile < 8 else 1\n",
    "\n",
    "            if next_tile != correct_successor:\n",
    "                S += 2\n",
    "\n",
    "    if state[4] != 0:\n",
    "        S += 1\n",
    "\n",
    "    h = P + 3*S\n",
    "\n",
    "    return h, P, S\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 39,
   "id": "bcb3bfad-0810-4fb1-87dd-0620d8036866",
   "metadata": {},
   "outputs": [],
   "source": [
    "### successors states generator\n",
    "\n",
    "def neighbors(state):\n",
    "    neighbors = []\n",
    "\n",
    "    blank_index = state.index(0)\n",
    "    row = blank_index // 3\n",
    "    col = blank_index % 3\n",
    "\n",
    "    moves = [(-1,0), (1,0), (0,-1), (0,1)] #Possible moves: up, down, left, right\n",
    "\n",
    "    for dr, dc in moves:\n",
    "        new_row = row + dr\n",
    "        new_col = col + dc\n",
    "\n",
    "        if 0 <= new_row < 3 and 0 <= new_col < 3:\n",
    "            new_index = new_row * 3 + new_col\n",
    "\n",
    "            new_state = list(state)  # convert tuple to list\n",
    "            new_state[blank_index], new_state[new_index] = \\\n",
    "                new_state[new_index], new_state[blank_index]\n",
    "\n",
    "            neighbors.append(tuple(new_state))  # convert back to tuple\n",
    "\n",
    "    return neighbors\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "id": "7b79fa84-d3fb-4bab-badc-25b07426783c",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Node:\n",
    "    def __init__(self, state, parent=None, g=0, h=0):\n",
    "        self.state = state\n",
    "        self.parent = parent\n",
    "        self.g = g\n",
    "        self.h = h\n",
    "        self.f = g + h"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "fd52f16d-8d26-444e-a135-737c04f4bf19",
   "metadata": {},
   "outputs": [],
   "source": [
    "def reconstruct_path(node):\n",
    "    path = []\n",
    "    while node:\n",
    "        path.append(node)\n",
    "        node = node.parent\n",
    "    return path[::-1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "007d2ce6-583a-4290-8ff9-507df4630885",
   "metadata": {},
   "outputs": [],
   "source": [
    "#### A* Algorithm\n",
    "\n",
    "def astar(start, goal, heuristic):\n",
    "\n",
    "    open_list = []\n",
    "    closed_set = set()\n",
    "\n",
    "    # Compute heuristic for start\n",
    "    h_value = heuristic(start, goal)\n",
    "\n",
    "    # Handle Nilsson returning 3 values\n",
    "    if isinstance(h_value, tuple):\n",
    "        h_value = h_value[0]\n",
    "\n",
    "    start_node = Node(start, None, 0, h_value)\n",
    "    open_list.append(start_node)\n",
    "\n",
    "    nodes_generated = 1\n",
    "\n",
    "    while open_list:\n",
    "\n",
    "        # Select node with smallest f\n",
    "        current = min(open_list, key=lambda node: node.f)\n",
    "        open_list.remove(current)\n",
    "\n",
    "        # Goal test\n",
    "        if current.state == goal:\n",
    "            return reconstruct_path(current), nodes_generated\n",
    "\n",
    "        # Add to CLOSED\n",
    "        closed_set.add(current.state)\n",
    "\n",
    "        # Generate successors\n",
    "        for neighbor in neighbors(current.state):\n",
    "\n",
    "            if neighbor in closed_set:\n",
    "                continue\n",
    "\n",
    "            g_new = current.g + 1\n",
    "\n",
    "            h_value = heuristic(neighbor, goal)\n",
    "            if isinstance(h_value, tuple):\n",
    "                h_value = h_value[0]\n",
    "\n",
    "            neighbor_node = Node(neighbor, current, g_new, h_value)\n",
    "\n",
    "            nodes_generated = nodes_generated + 1\n",
    "\n",
    "            # check if node is already in OPEN\n",
    "            existing = next((n for n in open_list if n.state == neighbor), None)\n",
    "\n",
    "            if existing is None:\n",
    "                open_list.append(neighbor_node)\n",
    "            elif g_new < existing.g:\n",
    "                existing.g = g_new\n",
    "                existing.f = g_new + existing.h\n",
    "                existing.parent = current\n",
    "\n",
    "    return None, nodes_generated"
   ]
